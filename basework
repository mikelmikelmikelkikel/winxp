local function EWAPVR_fake_script() -- Home.HomeScript 
	local script = Instance.new('LocalScript', Home)

	local Menus = script.Parent.Parent.Parent:WaitForChild("Menus")
	
	function leftClick()
		script.Parent.Parent.Parent.SoundScript:WaitForChild("ClickSound"):Play()
		Menus.Welcome.Visible = true
		Menus.Character.Visible = false
		Menus.Scripts.Visible = false
		Menus.HatScripts.Visible = false
		Menus.AudioLogger.Visible = false
	end 
	
	script.Parent.MouseButton1Click:Connect(leftClick)
	
end
coroutine.wrap(EWAPVR_fake_script)()
local function XUIUN_fake_script() -- Arrow.OpenScript 
	local script = Instance.new('LocalScript', Arrow)

	local Open = false
	
	function leftClick()
		script.Parent.Image = "rbxassetid://6531531478"
		script.Parent.Parent.Parent.SoundScript:WaitForChild("ClickSound"):Play()
		if Open == false  
		then script.Parent.Parent.Buttons:TweenPosition(UDim2.new(0, 0,0.062, 0), 'Out', 'Bounce', '.5')
			Open = true
		else
			script.Parent.Image = "rbxassetid://6531531599"
			script.Parent.Parent.Parent.SoundScript:WaitForChild("ClickSound"):Play()
			Open = false
			script.Parent.Parent.Buttons:TweenPosition(UDim2.new(1, 0,0.062, 0), 'Out', 'Bounce', '.5')
		end
	end
	
	
	script.Parent.MouseButton1Click:Connect(leftClick)
	
end
coroutine.wrap(XUIUN_fake_script)()
