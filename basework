local function HVDZNV_fake_script() -- randomizedimage.randomized; 
	local script = Instance.new('LocalScript', randomizedimage)

	local Images = {"rbxassetid://4759703774", "rbxassetid://1505144534", "rbxassetid://2663223907", "rbxassetid://5666154615", "rbxassetid://1639707845", "rbxassetid://1315317859", "rbxassetid://6464730351", "rbxassetid://6494652399", "rbxassetid://6437395672", "rbxassetid://6425796280", "rbxassetid://6075395846", "rbxassetid://5584500731", "rbxassetid://5610942639", "rbxassetid://5160712341", "rbxassetid://6549351471", "rbxassetid://6550268109", "rbxassetid://5655028554", "rbxassetid://4933468761", "rbxassetid://5710497007", "rbxassetid://5741657918", "rbxassetid://5768229099", "rbxassetid://5898091102", "rbxassetid://5952824955", "rbxassetid://5947794308", "rbxassetid://6032841223", "rbxassetid://4806105685", "rbxassetid://6093779811", "rbxassetid://6117941170", "rbxassetid://6116415183", "rbxassetid://6117966004", "rbxassetid://6109937685", "rbxassetid://5460374612", "rbxassetid://6165969352", "rbxassetid://6002645804", "rbxassetid://6197068901", "rbxassetid://6370900679", "rbxassetid://6243555061", "rbxassetid://6251050057", "rbxassetid://374838431", "rbxassetid://6523940003", "rbxassetid://6447447883"}
	while true do 
		script.Parent.Image = Images[math.random(1,#Images)]
		wait(6.9) 
	end
end
coroutine.wrap(HVDZNV_fake_script)()
local function PKTUU_fake_script() -- plrLeaderboardTemplate.handler 
	local script = Instance.new('LocalScript', plrLeaderboardTemplate)

	
	local plr = game.Players.LocalPlayer
	
	
	script.Parent:GetPropertyChangedSignal("Name"):Connect(function()
		
		victimName = script.Parent.Name
		print(victimName)
		
		script.Parent.plrImage.Image = 'https://www.roblox.com/bust-thumbnail/image?userId=' .. game.Players[victimName].UserId .. '&width=420&height=420&format=png'
		
	end)
	
	
	script.Parent.teleport.MouseButton1Click:connect(function()
		
		local desiredPlrPosition = game.Workspace[victimName].HumanoidRootPart.CFrame
		
		plr.Character:SetPrimaryPartCFrame(desiredPlrPosition + Vector3.new(0, 0, 3))
		
	end)
	
	
	
	currentlySpectating = false
	
	script.Parent.spectate.MouseButton1Click:connect(function()
		local cam = game.Workspace.CurrentCamera
		if currentlySpectating == false then 
			local victomModel = game.Workspace[victimName]
			cam.CameraSubject = victomModel.Head
			cam.CameraType = "Follow"
			currentlySpectating = true
			
			for i, v in ipairs(script.Parent.Parent:GetDescendants()) do
				if v.Name == 'spectate' then v.Image = 'rbxassetid://7019612109'
			end end
				
			script.Parent.spectate.Image = 'rbxassetid://7019611999'
			
		else
			
			cam.CameraSubject = plr.Character:FindFirstChild("Humanoid")
			cam.CameraType = "Custom"
			currentlySpectating = false
			
			script.Parent.spectate.Image = 'rbxassetid://7019612109'
			
		end
	
	end)
	
	
	
	
	
end
coroutine.wrap(PKTUU_fake_script)()
